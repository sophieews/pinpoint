{
  "expo": {
    "name": "PinPoint",
    "description": "A very interesting project.",
    "slug": "pinpoint",
    "privacy": "public",
    "sdkVersion": "29.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/pin.png",
    "splash": {
      "image": "./assets/images/pinpoint-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pinpoint.app"
    },
    "android": {
      "package": "com.pinpoint.app",
      "permissions": [
        "CAMERA",
        "CAMERA_ROLL"
      ]
    }
  }
}